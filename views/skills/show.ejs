<%- include('../partials/header') %>

  <h1><%= skill.skill %></h1>
  <h3>Skill Level: <%= skill.level %></h3>
  <h3>Example Project: <%= skill.project %></h3>
  <a href="http://localhost:3000/skills/<%= skill.id %>/edit">Update Skill</a>
  <br>
  <br>
  <form action="/skills/<%= skill.id %>?_method=DELETE"
    class="delete-form" method="POST">
    <button type="submit">Delete Skill</button>
  </form>
  <br>
  <a href="http://localhost:3000/skills/">Back to Developer Skills</a>
  
<%- include('../partials/footer') %>